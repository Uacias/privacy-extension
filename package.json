{
	"name": "privacy-extension",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build --config vite.config.bg.ts && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/chrome": "^0.0.310",
		"@types/node": "^22.13.13",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-adapter-chrome-extension": "^2.0.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vite-plugin-static-copy": "^2.3.0"
	},
	"dependencies": {
		"@aztec/bb.js": "0.67.0",
		"@noir-lang/acvm_js": "^1.0.0-beta.1",
		"@noir-lang/noir_js": "1.0.0-beta.1",
		"@noir-lang/noir_wasm": "1.0.0-beta.1",
		"@noir-lang/noirc_abi": "^1.0.0-beta.1",
		"@scure/bip39": "^1.5.4",
		"garaga": "v0.15.5"
	}
}
